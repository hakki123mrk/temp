{
  "name": "whatsapp-commerce-webhook",
  "version": "1.0.0",
  "description": "Webhook server for WhatsApp Commerce orders",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy": "cd cdk && npm install && cdk bootstrap && cdk deploy --require-approval never",
    "cdk:synth": "cd cdk && cdk synth",
    "cdk:diff": "cd cdk && cdk diff",
    "cdk:destroy": "cd cdk && cdk destroy"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.4.0",
    "serverless-http": "^3.2.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
